<h1><%= @event.title %></h1>
<p><strong>Location:</strong> <%= @event.location %></p>
<p><strong>Date</strong> <%= @event.date %></p>
<p><strong>Created by:</strong> <%= @event.creator.name %></p>
<h3>Attendees:</h3>
<ul>
  <% @event.attendees.each do |a| %>
    <li><%= a.name %></li>
  <% end %>
</ul>


